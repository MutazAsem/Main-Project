{
    "name": "travelx/school",
    "description": "Travel X School Packge",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Travelx\\School\\": "src",
            "Travelx\\School\\Tests\\": "tests",
            "Travelx\\School\\Database\\Factories\\": "database/factories"
        }
    },
    "authors": [
        {
            "name": "Mutaz"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Travelx\\School\\SchoolServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "3.x-dev"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
